{
  "name": "vue-route-prefetch",
  "version": "3.0.0",
  "description": "A Vue plugin to prefetch the route components.",
  "license": "MIT",
  "author": "Aaron Zhou",
  "packageManager": "pnpm@8.9.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "keywords": [
    "vue",
    "prefetch",
    "preload",
    "vue-router",
    "prefetch route",
    "prefetch page",
    "quicklink"
  ],
  "sideEffects": false,
  "homepage": "https://github.com/Clarkkkk/vue-router-prefetch#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Clarkkkk/vue-router-prefetch"
  },
  "bugs": {
    "url": "https://github.com/Clarkkkk/vue-router-prefetch/issues"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "test": "npm run test:e2e",
    "build": "tsc && vite build",
    "example": "vite example",
    "commit": "git-cz",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test:e2e": "run-p --race example cy:run",
    "typecheck": "tsc --noEmit --watch --preserveWatchOutput",
    "format": "eslint --fix --cache {src,example}/**/*.{vue,ts}",
    "prepare": "husky install",
    "uninstall-husky": "npm uninstall husky --no-save && git config --unset core.hooksPath && npx rimraf .husky",
    "changelog": "conventional-changelog -o CHANGELOG.md -p aaron-preset -r 0",
    "release": "commit-and-tag-version"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/tsconfig": "^0.4.0",
    "commit-and-tag-version": "^11.3.0",
    "conventional-changelog": "^5.1.0",
    "conventional-changelog-aaron-preset": "^1.0.4",
    "conventional-changelog-cli": "^4.1.0",
    "cypress": "^13.3.2",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "rollup-plugin-node-externals": "^6.1.2",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-dts": "^3.6.0",
    "vitest": "^0.34.6",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5"
  },
  "peerDependencies": {
    "vue": "^3",
    "vue-router": "^4"
  }
}
