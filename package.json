{
  "name": "vue-router-prefetch",
  "version": "1.0.0",
  "description": "Prefetch the route components of router-link in viewport.",
  "author": "egoist<0x142857@gmail.com>",
  "license": "MIT",
  "repository": {
    "url": "egoist/vue-router-prefetch",
    "type": "git"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "test": "npm run test:e2e",
    "lint": "xo",
    "build": "bili --format esm,cjs,esm-min --minimal",
    "prepublishOnly": "npm run build",
    "example": "vite example",
    "commit": "git-cz",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test:e2e": "run-p --race example cy:run",
    "typecheck": "tsc --noEmit --watch --preserveWatchOutput",
    "format": "eslint --fix --cache {src,example}/**/*.{vue,ts}",
    "prepare": "husky install",
    "postinstall": "nuxt prepare",
    "uninstall-husky": "npm uninstall husky --no-save && git config --unset core.hooksPath && npx rimraf .husky",
    "changelog": "conventional-changelog -o CHANGELOG.md -p aaron-preset -r 0",
    "release": "commit-and-tag-version"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/tsconfig": "^0.4.0",
    "bili": "^5.0.5",
    "commit-and-tag-version": "^11.3.0",
    "commitizen": "^4.3.0",
    "conventional-changelog": "^5.1.0",
    "conventional-changelog-aaron-preset": "^0.2.2",
    "conventional-changelog-cli": "^4.1.0",
    "cypress": "^13.3.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "rollup-plugin-node-externals": "^6.1.2",
    "semantic-release": "^22.0.5",
    "vite": "^4.5.0",
    "vite-plugin-dts": "^3.6.0",
    "vitest": "^0.34.6",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5",
    "xo": "^0.56.0"
  },
  "peerDependencies": {
    "vue": "^3",
    "vue-router": "^4"
  }
}
